{
  "sourceType": "unambiguous",
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "add-module-exports",
    ["@babel/plugin-transform-runtime", {
      "regenerator": true
    }]
  ],
  "env": {
    "esm": {
      "sourceType": "unambiguous",
      "presets": [
        [
          "@babel/preset-env",
          {
            "loose": true,
            "modules": false
          }
        ],
        "@babel/preset-react"
      ],
      "plugins": [
        "./scripts/babel-plugin-fully-specified-relative-imports.cjs",
        ["@babel/plugin-transform-runtime", {
          "regenerator": true
        }]
      ]
    }
  }
}
